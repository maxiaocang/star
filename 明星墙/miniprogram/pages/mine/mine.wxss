.avatar{
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  overflow: hidden;
  margin: 30rpx auto;
}
.avatar image{
  width: 100%;
  height: 100%;
}
.username{
  text-align: center;
  color: #aaa
}
.main{
  margin-top: 20rpx;
  border-top: 30rpx solid #DBDBDB;
}
.main .list{
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 2px solid #DBDBDB;
  position: relative;
  padding-left: 130rpx;
}
.main .list image{
  position: absolute;
  top: 0;
  bottom: 0;
  margin: auto;
  left: 50rpx;
  width: 50rpx;
  height: 50rpx;
}
