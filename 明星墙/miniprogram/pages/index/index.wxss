page{
  display: flex;
  flex-wrap: wrap;
  background: #DBDBDB;
  padding: 0 10rpx;
}
.user_list{
  width: 350rpx;
  padding-bottom: 10rpx; 
  margin: 15rpx 0 15rpx 10rpx;
  background: #fff;
}
.user_list .pic{
  width: 350rpx;
  height: 400rpx;
}
.user_list .pic image{
  width: 100%;
  height: 100%;
}
.user_list .dsc{
  width: 330rpx;
  height: 80rpx;
  margin: 0 auto;
   margin-top: 30rpx;
  font-size: 14px;
  text-indent: 2em;
  /* 强制显示两行，超出显示省略号 */
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  overflow:hidden;
  /*! autoprefixer: off */
  -webkit-box-orient: vertical;
}
.user_list .user{
  padding: 30rpx 0 20rpx 20rpx;
}
.user_list .user image{
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  vertical-align: middle;
  margin-right: 10rpx;
}
.user_list .user{
  color: #ccc;
  font-size: 14px;
}